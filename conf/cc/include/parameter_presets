   --mysqld=--loose-s3-secret-key=minioadmin
   --mysqld=--loose-s3-region=""
   --mysqld=--loose-s3-port=9000
   --mysqld=--loose-s3-use-http=on
   --grammar=conf/yy/engine-s3.yy:0.1
  ',
];

