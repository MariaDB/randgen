# PS protocol
#++++++++++++++++++++++++

$optional_ps_protocol{$minversion}= [
  @empty_set_10,
  '--ps-protocol --filter=conf/ff/need-reconnect.ff'
];

$parameters{optional_ps_protocol}= \%optional_ps_protocol;

