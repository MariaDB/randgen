    '--mysqld=--innodb-adaptive-hash-index-parts=2',
    '--mysqld=--innodb-adaptive-hash-index-parts=4',
    '--mysqld=--innodb-adaptive-hash-index-parts=8',
    '--mysqld=--innodb-adaptive-hash-index-parts=16',
    '--mysqld=--innodb-adaptive-hash-index-parts=64',
    '--mysqld=--innodb-adaptive-hash-index-parts=512'
  ],
  # Default 1
  [ @empty_set_30,
    '--mysqld=--innodb-autoinc-lock-mode=0',
    '--mysqld=--innodb-autoinc-lock-mode=1',
    '--mysqld=--innodb-autoinc-lock-mode=2',
  ],
  # Default ON
